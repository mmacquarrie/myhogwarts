<div class="container">
  <div class="row">
    <div class="col-md-5">
      <img src="<%= image_url('%s_logo.png' % [@professor.house.downcase])%>" class="profile-logo">
    </div>
    <div class="col-md-5">
      <h2><%= @professor.first_name %> <%= @professor.last_name %></h2>
      <h4>Currently Teaching:</h4>
      <ul>
        <% @professor.hogwarts_classes.each() do |hogwarts_class|  %>
         <li><%= hogwarts_class.course.course_name %></li>
        <% end %>
      </ul>
    </div>
  </div>
</div>
